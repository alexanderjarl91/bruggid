35, -18.356791755646736]}\n          icon={beerIcon}\n        >\n          {/* Breyta staðsetningu á popup miðað við marker */}\n          <Popup>\n            <strong>Bruggsmiðjan Kaldi Brewery</strong> <br /> Öldugata 22, 621\n            Árskógssandur\n          </Popup>\n        </Marker>\n        <LocationMarker />\n      </MapContainer>\n      <Nav />\n    </div>\n  );\n}\n\nexport default Map;\n","/Users/alexanderjarl/Desktop/iceAle/bruggid/src/reportWebVitals.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/Beer.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/Brewery.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/BeerListItem.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/BeerList.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/Sort.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/styled/index.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/Nav.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/styled/navStyled.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/styled/ListStyled.js",[],"/Users/alexanderjarl/Desktop/iceAle/bruggid/src/components/styled/listStyled.js",[],{"ruleId":"69","severity":1,"message":"70","line":19,"column":10,"nodeType":"71","messageId":"72","endLine":19,"endColumn":19},{"ruleId":"69","severity":1,"message":"73","line":22,"column":10,"nodeType":"71","messageId":"72","endLine":22,"endColumn":27},{"ruleId":"74","severity":1,"message":"75","line":26,"column":13,"nodeType":"76","endLine":31,"endColumn":4},{"ruleId":"69","severity":1,"message":"77","line":1,"column":17,"nodeType":"71","messageId":"72","endLine":1,"endColumn":25},{"ruleId":"69","severity":1,"message":"78","line":1,"column":27,"nodeType":"71","messageId":"72","endLine":1,"endColumn":36},"no-unused-vars","'breweries' is assigned a value but never used.","Identifier","unusedVar","'filteredBreweries' is assigned a v
st [expanded, setExpanded] = useState(false)\n  useEffect(() => {\n    setExpanded(null);\n  }, [data]);\n  return (\n    <>\n      {/* ATH: value-in verða að hafa if statement á length því annars reynir react að rendera áður en hann fetchar og þá crashar síðan */}\n      {data.map((beer, i) => (\n        <ListItem key={beer.name} className=\"brewlistitem__brewery\">\n          <StyledBeerItem onClick={ () => setExpanded(i === expanded ? null : i)  }>\n            <StyledBeerInfo>\n              <ListImage src={beer.img} alt=\"\" />\n              <NameAndTypeContainer>\n                <Name>{beer.name}</Name>\n                <Type>{`${beer.abv} / ${beer.vol} / ${beer.type}`}</Type>\n              </NameAndTypeContainer>\n            </StyledBeerInfo>\n            <StyledArrow className={expanded === i ? 'arrowUp' : 'arrowDown'}/>\n          </StyledBeerItem>\n          \n          <StyledBeerDropdown className={expanded === i ? 'expanded' : 'collapsed'}>\n          <Type>{beer.about}</Type>\n            <span>\n              <a href={`/breweries/${beer.brewery}`}><Type>{beer.brewery}</Type></a>\n            </span>\n          </StyledBeerDropdown>\n        </ListItem>\n      ))}\n    </>\n  );\n}\n\nexport default BeerListItem;\n","/Users/jonthorarnason/Desktop/Vefthroun/Module4/brugg/bruggid/src/components/BreweryDetails.js",[],"/Users/jonthorarnason/Desktop/Vefthroun/Module4/brugg/bruggid/src/components/Map.js",["56","57","58"],"/Users/jonthorarnason/Desktop/Vefthroun/Module4/brugg/bruggid/src/components/styled/listStyled.js",[],"/Users/jonthorarnason/Desktop/Vefthroun/Module4/brugg/bruggid/src/components/BreweryListItem.js",["59"],{"ruleId":"60","severity":1,"message":"61","line":10,"column":3,"nodeType":"62","messageId":"63","endLine":10,"endColumn":13},{"ruleId":"60","severity":1,"message":"64","line":14,"column":10,"nodeType":"62","messageId":"63","endLine":14,"endColumn":17},{"ruleId":"60","severity":1,"message":"65","line":15,"column":8,"nodeType":"62","m